<div class="swiper aeneid-group-slider col-span-full w-full">
  <div class="swiper-wrapper aeneid-slider-collections">
    {% for i in (1..15) %}
      <div class="swiper-slide">
        <div class="slider-collection-image">
          <img  class="c"
                src="https://images.unsplash.com/photo-1661956602926-db6b25f75947?ixlib=rb-4.0.3&ixid=MnwxMjA3fDF8MHxlZGl0b3JpYWwtZmVlZHwxfHx8ZW58MHx8fHw%3D&auto=format&fit=crop&w=500&q=60" alt="product">
        </div>
        <div class="slider-collections-content">
          <div class="text-body-large mb-3 md:mb-4">
            Blankets{{ i }}
          </div>
          <div class="slider-collections-label">
            DO YOU LIKE BLANKETS OR NO?
          </div>
          <a href="#" class="button w-full button-hover-inverse">
            See every color
            <svg width="24" height="24" viewBox="0 0 24 24" class="ml-6" style="" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path class="group/edit group-hover/item:stroke-black" d="M7 17L16.3252 7.6748" stroke="currentColor" stroke-width="2" stroke-linecap="square"></path>
              <path class="group/edit group-hover/item:stroke-black" d="M7 7H17V17" stroke="currentColor" stroke-width="2" stroke-linecap="square"></path>
            </svg>
          </a>
        </div>
      </div>
    {% endfor %}
  </div>
</div>